    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-test-renderer": "^16.3.0",
    "reify": "^0.18.1",
    "sinon": "4.1.3",
    "tap-browser-color": "^0.1.2",
    "tape": "^4.5.1",
    "tape-catch": "^1.0.4",
    "webpack": "^4.3.0",
    "webpack-bundle-analyzer": "^3.3.2",
