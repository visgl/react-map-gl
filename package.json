{
  "name": "react-map-gl",
  "description": "A React wrapper for MapboxGL-js and overlay API.",
  "version": "1.7.2",
  "main": "index.js",
  "module": "dist/index.js",
  "keywords": [
    "mapbox",
    "mapbox-gl",
    "react",
    "react-mapbox-gl",
    "react mapbox"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/uber/react-map-gl.git"
  },
  "license": "MIT",
  "files": [
    "src",
    "dist",
    "bundle.js"
  ],
  "dependencies": {
    "bowser": "^1.2.0",
    "d3-selection": "^1.0.2",
    "global": "^4.3.0",
    "mapbox-gl": "0.26.0",
    "react-addons-shallow-compare": "^15.4.2",
    "viewport-mercator-project": "^2.0.1"
  },
  "devDependencies": {
    "buble": "^0.15.1",
    "disc": "^1.3.2",
    "electron-prebuilt": "^0.37.3",
    "envify": "^3.4.0",
    "eslint": "^3.0.0",
    "eslint-config-uber-es2015": "^3.0.0",
    "eslint-config-uber-jsx": "^3.0.0",
    "eslint-plugin-react": "~6.7.0",
    "immutable": "^3.7.6",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "tap-browser-color": "^0.1.2",
    "tape": "^4.5.1",
    "tape-catch": "^1.0.4",
    "testron": "^1.2.0",
    "webpack": "^2.2.0-rc.3"
  },
  "peerDependencies": {
    "immutable": "^3.7.6",
    "react": "0.14.x - 15.x"
  },
  "scripts": {
    "build": "buble src -o dist --no modules --y dangerousForOf --objectAssign && webpack -d",
    "start": "(cd example && yarn && npm start)",
    "lint": "eslint src example test",
    "test": "npm run lint && npm run build && cat dist/bundle.js | testron",
    "precommit": "npm run lint -s",
    "prepush": "npm run lint -s",
    "publish-prod": "npm run build && npm run test-fast && npm publish",
    "publish-beta": "npm run build && npm run test-fast && npm publish --tag beta"
  },
  "engines": {
    "node": "6.x",
    "npm": "3.x"
  }
}
