{
  "name": "react-map-gl",
  "description": "A React wrapper for MapboxGL-js and overlay API.",
  "version": "2.0.0-alpha.1",
  "keywords": [
    "mapbox",
    "mapbox-gl",
    "react",
    "react-mapbox-gl",
    "react mapbox"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/uber/react-map-gl.git"
  },
  "license": "MIT",
  "main": "index.js",
  "module": "dist/index.js",
  "files": [
    "src",
    "dist"
  ],
  "dependencies": {
    "bowser": "^1.2.0",
    "d3-selection": "^1.0.2",
    "global": "^4.3.0",
    "mapbox-gl": "0.29.0",
    "react-addons-shallow-compare": "^15.4.2",
    "viewport-mercator-project": "^2.0.1"
  },
  "devDependencies": {
    "buble": "^0.15.1",
    "buble-loader": "^0.4.0",
    "eslint": "^3.0.0",
    "eslint-config-uber-es2015": "^3.0.0",
    "eslint-config-uber-jsx": "^3.0.0",
    "eslint-plugin-react": "~6.7.0",
    "gl": "^4.0.2",
    "immutable": "^3.7.6",
    "jsdom": "^9.9.1",
    "module-alias": "^2.0.0",
    "pre-commit": "^1.2.2",
    "react": "^15.4.0",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.0",
    "reify": "^0.4.4",
    "sinon": "^1.17.7",
    "tap-browser-color": "^0.1.2",
    "tape": "^4.5.1",
    "tape-catch": "^1.0.4",
    "webpack": "^2.2.0-rc.3"
  },
  "peerDependencies": {
    "immutable": "^3.7.6",
    "react": "0.14.x - 15.x"
  },
  "scripts": {
    "start": "(cd examples/main && yarn && npm start)",
    "build": "rm -fr dist && buble src -o dist --no modules --y dangerousForOf --objectAssign && webpack -d",
    "lint": "eslint src example test",
    "test": "npm run lint && npm run test-node",
    "test-node": "npm run build && node test/node.js",
    "test-browser": "npm run build && echo 'To be restored'",
    "publish-prod": "npm run build && npm run test && npm publish",
    "publish-beta": "npm run build && npm run test && npm publish --tag beta"
  },
  "engines": {
    "node": "6.x",
    "npm": "3.x"
  }
}
