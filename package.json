  "version": "4.1.4",
  "keywords": [
    "mapbox",
    "mapbox-gl",
    "react",
    "react-mapbox-gl",
    "react mapbox"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/uber/react-map-gl.git"
  },
  "license": "MIT",
  "main": "dist/es5/index.js",
  "module": "dist/esm/index.js",
  "esnext": "dist/es6/index.js",
  "browser": {
    "./dist/es5/utils/mapboxgl.js": "./dist/es5/utils/mapboxgl.browser.js",
