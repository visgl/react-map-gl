{
  "compilerOptions": {
    "target": "es2018",
    "module": "commonjs",
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "noEmit": true
  },
  "include":[
    "examples/**/*",
    "src/**/*",
    "test/**/*"
  ]
}
